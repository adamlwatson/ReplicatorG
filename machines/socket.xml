<?xml version="1.1" encoding="utf-8"?>
<machines>
    <!--  Note: Socket machines don't really need most of this info, it's handled by the remote server. -->
	<machine>
		<name>Remote Thingomatic w/ HBP and Stepstruder MK6</name>
		<geometry type="cartesian">
			<!-- different pulleys on X and Y axii -->
			<axis id="x" length="106" maxfeedrate="4000" homingfeedrate="500" stepspermm="1" endstops="min"/>  <!-- Pulley dia: 10.82mm / 1/8 step = 1/(10.82 * pi / 1600) -->
			<axis id="y" length="120" maxfeedrate="4000" homingfeedrate="500" stepspermm="1" endstops="min"/>  <!-- Pulley dia: 10.82mm / 1/8 step = 1/(10.82 * pi / 1600) -->
			<axis id="z" length="106" maxfeedrate="1000" homingfeedrate="500" stepspermm="1" endstops="max"/> <!-- TR-8x8 Z axis = 1/(8/1600) -->
			<axis id="a" length="100000" maxfeedrate="1600" stepspermm="1" endstops="none"/> <!-- stepspermm is incoming filament length, 127 is ca. 4 RPM, 1600 ca. 50 RPM -->
		</geometry>
		<tools>
			<tool name="Stepstruder MK6" type="extruder" material="abs" motor="true" fan="true" heatedplatform="true" motor_steps="1" default_rpm="1.98" heater="true" stepper_axis="a"/>
		</tools>
		<clamps></clamps>
		<driver name="socket">
		</driver>
		<warmup>
		</warmup>
		<cooldown>
		</cooldown>
	</machine>
</machines>
